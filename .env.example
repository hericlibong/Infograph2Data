# Environment Variables
# Copy this file to .env and fill in values

# Debug mode (set to true for development)
DEBUG=false

# CORS origins (comma-separated, for frontend access)
# CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# OpenAI API Key (required for Vision LLM extraction)
OPENAI_API_KEY=sk-your-api-key-here

# OpenAI Model (default: gpt-4o)
OPENAI_MODEL=gpt-4o

# Vision LLM timeout in seconds
VISION_TIMEOUT=30

# Identification TTL in seconds (how long before identification expires)
IDENTIFICATION_TTL=3600
